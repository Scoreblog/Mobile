<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--  Cores Definidas  -->
    <Color x:Key="PrimaryColor">#F75105</Color>
    <Color x:Key="SecondaryColor">#F78E21</Color>
    <Color x:Key="LightGray">#D2D3D4</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="DarkGray">#5D5C5B</Color>
    <Color x:Key="DarkerGray">#2F323A</Color>
    <Color x:Key="AlmostBlack">#22242B</Color>
    <Color x:Key="White">#FFFFFF</Color>

    <!--  Brushes Correspondentes  -->
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="LightGrayBrush" Color="{StaticResource LightGray}" />
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}" />
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGray}" />
    <SolidColorBrush x:Key="DarkerGrayBrush" Color="{StaticResource DarkerGray}" />
    <SolidColorBrush x:Key="AlmostBlackBrush" Color="{StaticResource AlmostBlack}" />
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}" />
</ResourceDictionary>